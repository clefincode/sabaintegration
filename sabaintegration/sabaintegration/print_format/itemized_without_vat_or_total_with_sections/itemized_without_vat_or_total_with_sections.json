 "html": "\r\n {% set quotation = doc.name %}\r\n <div class=\"print-heading\">\r\n     <div class=\"letter-head\" style=\"\">\r\n    \t{{ frappe.render_template(frappe.db.get_value(\"Letter Head\",\"SABA Default\", \"content\"), {\"doc\": doc}) }}\r\n    </div>\r\n     <h2>\r\n        <div>Quotation</div><br><small class=\"sub-heading\">{{ doc.name }}</small>\r\n    </h2>\r\n</div>\r\n\r\n\r\n <!--<div style=\"border-top:1px solid black; padding-bottom:6px;\"></div>-->\r\n <div class=\"row section-break\" data-label=\"\">\r\n            {%- if doc.customer_name -%}\r\n\t\t\t<div class=\"col-xs-6 column-break\">\r\n    \t\t\t<div class=\"row important data-field\" data-fieldname=\"customer_name\" data-fieldtype=\"Data\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Customer Name: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\" id=\"customer\">\r\n    \t\t\t\t\t{{doc.customer_name}}\r\n    \t\t\t\t</div>\r\n    \t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t{%- endif -%}\r\n\t\t\t<div class=\"col-xs-6 column-break\">\r\n\t\t\t    \r\n\t\t\t    \r\n\t\t\t    {%- if doc.amended_from -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"transaction_date\" data-fieldtype=\"Date\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Amended From: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">\r\n    \t\t\t\t\t{{doc.amended_from}}\r\n    \t\t\t\t</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n    \t\t\t{%- if doc.transaction_date -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"valid_till\" data-fieldtype=\"Date\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Date: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">\r\n    \t\t\t\t\t{{doc.transaction_date}}\r\n    \t\t\t\t</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n    \t\t\t{%- if doc.valid_till -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"valid_till\" data-fieldtype=\"Date\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Valid Till: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">\r\n    \t\t\t\t\t{{doc.valid_till}}\r\n    \t\t\t\t</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n    \t\t\t{%- if doc.vendor_ope_or_qtn_number -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"valid_till\" data-fieldtype=\"Date\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Vendor OPE/QTN number: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">\r\n    \t\t\t\t\t{{doc.vendor_ope_or_qtn_number}}\r\n    \t\t\t\t</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n\t\t\t</div>\r\n\t</div>\r\n<div class=\"row section-break\" data-label=\"Address and Contact\">\r\n\t\t\t<div class=\"col-xs-6 column-break\">\r\n\t\t\t    {%- if doc.address_display -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Address: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">{{doc.address_display}}</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n    \t\t\t{%- if doc.contact_display -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Contact: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">{{doc.contact_display}}</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-6 column-break\">\r\n\t\t\t    {%- if doc.company_address -%}\r\n\t\t\t    <div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Company Address Name: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">{{doc.company_address}}</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n    \t\t\t{%- if doc.company_address_display -%}\r\n    \t\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n    \t\t\t\t<div class=\"col-xs-5\">\r\n    \t\t\t\t\t<label>Company Address: </label>\r\n    \t\t\t\t</div>\r\n    \t\t\t\t<div class=\"col-xs-7 value\">{{doc.company_address_display}}</div>\r\n    \t\t\t</div>\r\n    \t\t\t{%- endif -%}\r\n\t\t\t</div>\r\n\t</div>\r\n <hr>\r\n <table class=\"table table-sm table-bordered table-condensed\">\r\n    <thead>\r\n        <tr>\r\n        <th scope=\"col\">Item Code</th> \r\n        <th scope=\"col\">Description</th>\r\n        <th scope=\"col\">Quantity</th>\r\n        <th scope=\"col\">Rate</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        {% set items1 = frappe.get_all('Quotation Item', filters={'parent': quotation}, fields=['idx','item_code', 'description','count(qty) as count' ,'sum(qty) as sumq','sum(rate) as sum_rate','section_title','amount','sum(amount) as amounts'],group_by='section_title', order_by='idx asc') %}\r\n\r\n        {% set section_titles = [] %}\r\n        \r\n        {%- for item in doc.items  if item.section_title not in section_titles -%}\r\n            {{ section_titles.append(item.section_title) or \"\" }}\r\n        {%- endfor -%}\r\n\r\n        {%- for section in section_titles   -%}\r\n            {% set outerloop = loop.index %}\r\n\r\n           \r\n                {%- for item in items1 if item.section_title == section  -%}\r\n                    {%- if section -%}\r\n                    <tr class=\"styleme{{outerloop}}\" style=\"background:#434141;color: white; text-align: center;\">\r\n                        <td colspan=\"5\" style=\"color: white; \"> {{section or '' }}</td>\r\n\r\n                    </tr>\r\n                     {%- endif -%}\r\n                {%- endfor -%}\r\n                \r\n                {%- for item in doc.items if item.section_title == section -%}\r\n                     <tr class=\"styleme{{outerloop}}\" >\r\n                        <td>{{ item.item_code }}</td>\r\n                        <td>{{ item.description }}</td>\r\n                        <td>{{ item.qty }}  {{ item.uom or item.stock_uom }}</td>\r\n                        <td> {{item.get_formatted('rate', doc) }} </td>\r\n                    </tr>\r\n                {%- endfor -%}\r\n                {%- for item in items1 if item.section_title == section  -%}\r\n                    <tr class=\"styleme{{outerloop}}\" style=\"background:#022856;color: white;text-align: right;\">\r\n                        <td colspan=\"5\" style=\"color: #ff9800; \">{{doc.currency}} {{ frappe.format(item.amounts )}}  </td>\r\n                    </tr>\r\n                {%- endfor -%}\r\n        {%- endfor -%}\r\n    </tbody>\r\n\r\n</table>\r\n\r\n\r\n{%- if doc.pricing_rules -%}\r\n<hr>\r\n<h3>Pricing Rule Detail</h3>\r\n<table class=\"table table-sm table-bordered table-condensed\" >\r\n   <thead>\r\n\t   <tr>\r\n\t\t<th scope=\"col\">Pricing Rule</th> \r\n\t   <th scope=\"col\">Item Code</th> \r\n\t   <th scope=\"col\">Margin Type</th>\r\n\t   <th scope=\"col\">Rate or Discount</th>\r\n\t   <th scope=\"col\">Rule Applied</th>\r\n\t   </tr>\r\n   </thead>\r\n   <tbody>\r\n\r\n\t{%- for rule in doc.pricing_rules  -%}\r\n\t\t<tr  >\r\n\t\t\t<td>{{ rule.pricing_rule }}</td>\r\n\t\t\t<td>{{ rule.item_code }}</td>\r\n\t\t\t<td>{{ rule.margin_type }}</td>\r\n\t\t\t<td>{{ rule.rate_or_discount }}</td>\r\n\t\t\t<td> {{rule.rule_applied }} </td>\r\n\t\t</tr>\r\n\t{%- endfor -%}\r\n   </tbody>\r\n</table>\r\n{%- endif -%}\r\n\r\n<div class=\"row section-break\" data-label=\"Address and Contact\">\r\n\t<div class=\"col-xs-6 column-break\">\r\n\t\t{%- if doc.coupon_code -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Coupon Code: </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\">{{doc.coupon_code}}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t\t{%- if doc.referral_sales_partner -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Referral Sales Partner: </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\">{{doc.referral_sales_partner}}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n\t<div class=\"col-xs-6 column-break\">\r\n\r\n\t</div>\r\n</div>\r\n<div class=\"row section-break\" data-label=\"\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t{%- if doc.sales_team_commission_template -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Sales Team Commission Template: </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\"> {{doc.sales_team_commission_template}}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n</div>\r\n{%- if doc.sales_team -%}\r\n<hr>\r\n<h3>Sales Team</h3>\r\n<table class=\"table table-sm  table-bordered table-condensed\" style=\"display: inline-block; overflow: auto;\">>\r\n   <thead>\r\n\t   <tr>\r\n\t    \t<th scope=\"col\">Sales Person</th> \r\n\t\t\t<th scope=\"col\">Commission Reason</th> \r\n\t\t\t<th scope=\"col\">Contact No.</th> \r\n\t\t\t<th scope=\"col\">Contribution (%)</th> \r\n\t\t\t<th scope=\"col\">Contribution to Net Total</th> \r\n\t\t\t<th scope=\"col\">Commission Rate</th> \r\n\t\t\t<th scope=\"col\">Incentives</th> \r\n\t\t\t<th scope=\"col\">First Payment</th> \r\n\t\t\t<th scope=\"col\">First Payment Date</th> \r\n\t   </tr>\r\n   </thead>\r\n   <tbody>\r\n\r\n\t{%- for rule in doc.sales_team  -%}\r\n\t\t<tr  >\r\n\t\t    <td>{{ rule.sales_person }}</td>\r\n\t\t\t<td>{{ rule.commission_reason }}</td>\r\n\t\t\t<td>{{ rule.contact_no }}</td>\r\n\t\t\t<td>{{ rule.allocate