 "html": "{% set quotation = doc.name %}\r\n<div class=\"print-heading\">\r\n    <h2>\r\n       <div>Quotation</div><br><small class=\"sub-heading\">{{ doc.name }}</small>\r\n   </h2>\r\n</div>\r\n\r\n\r\n<!--<div style=\"border-top:1px solid black; padding-bottom:6px;\"></div>-->\r\n<div class=\"row section-break\" data-label=\"\">\r\n           \r\n           <div class=\"col-xs-6 column-break\">\r\n               {%- if doc.customer_name -%}\r\n               <div class=\"row important data-field\" data-fieldname=\"customer_name\" data-fieldtype=\"Data\">\r\n                   <div class=\"col-xs-5\">\r\n                       <label>Customer Name: </label>\r\n                   </div>\r\n                   <div class=\"col-xs-7 value\" id=\"customer\">\r\n                       {{doc.customer_name}}\r\n                   </div>\r\n               </div>\r\n               {%- endif -%}\r\n               {%- if doc.contact_display -%}\r\n               <div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n                   <div class=\"col-xs-5\">\r\n                       <label>Contact: </label>\r\n                   </div>\r\n                   <div class=\"col-xs-7 value\">{{doc.contact_display}}</div>\r\n               </div>\r\n               {%- endif -%}\r\n           </div>\r\n           \r\n           <div class=\"col-xs-6 column-break\">\r\n               \r\n               \r\n               {%- if doc.amended_from -%}\r\n               <div class=\"row  data-field\" data-fieldname=\"transaction_date\" data-fieldtype=\"Date\">\r\n                   <div class=\"col-xs-5\">\r\n                       <label>Amended From: </label>\r\n                   </div>\r\n                   <div class=\"col-xs-7 value\">\r\n                       {{doc.amended_from}}\r\n                   </div>\r\n               </div>\r\n               {%- endif -%}\r\n               {%- if doc.transaction_date -%}\r\n               <div class=\"row  data-field\" data-fieldname=\"valid_till\" data-fieldtype=\"Date\">\r\n                   <div class=\"col-xs-5\">\r\n                       <label>Date: </label>\r\n                   </div>\r\n                   <div class=\"col-xs-7 value\">\r\n                       {{doc.transaction_date}}\r\n                   </div>\r\n               </div>\r\n               {%- endif -%}\r\n               {%- if doc.valid_till -%}\r\n               <div class=\"row  data-field\" data-fieldname=\"valid_till\" data-fieldtype=\"Date\">\r\n                   <div class=\"col-xs-5\">\r\n                       <label>Valid Till: </label>\r\n                   </div>\r\n                   <div class=\"col-xs-7 value\">\r\n                       {{doc.valid_till}}\r\n                   </div>\r\n               </div>\r\n               {%- endif -%}\r\n           </div>\r\n   </div>\r\n\r\n   <hr>\r\n <table class=\"table table-sm  table-bordered table-condensed\">\r\n    <thead>\r\n        <tr>\r\n        <th scope=\"col\">Item Code</th> \r\n        <th scope=\"col\">Description</th>\r\n        <th scope=\"col\">Quantity</th>\r\n        <th scope=\"col\">Stock UOM</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        {% set items1 = frappe.get_all('Quotation Item', filters={'parent': quotation}, fields=['idx','item_code', 'description','count(qty) as count' ,'sum(qty) as sumq','sum(rate) as sum_rate','section_title','amount','sum(amount) as amounts'],group_by='section_title', order_by='idx asc') %}\r\n\r\n        {% set section_titles = [] %}\r\n        \r\n        {%- for item in doc.items  if item.section_title not in section_titles -%}\r\n            {{ section_titles.append(item.section_title) or \"\" }}\r\n        {%- endfor -%}\r\n\r\n        {%- for section in section_titles   -%}\r\n            {% set outerloop = loop.index %}\r\n\r\n           \r\n                {%- for item in items1 if item.section_title == section  -%}\r\n                    {%- if section -%}\r\n                    <tr class=\"styleme{{outerloop}}\" style=\"background:#434141;color: white; text-align: center;\">\r\n                        <td colspan=\"5\" style=\"color: white; \"> {{section or '' }}</td>\r\n                    </tr>\r\n                     {%- endif -%}\r\n                {%- endfor -%}\r\n                \r\n                {%- for item in doc.items if item.section_title == section -%}\r\n                     <tr class=\"styleme{{outerloop}}\" >\r\n                        <td>{{ item.item_code }}</td>\r\n                        <td>{{ item.description }}</td>\r\n                        <td>{{ item.qty }} </td>\r\n\t\t\t\t\t\t<td> {{item.stock_uom }} </td>\r\n                    </tr>\r\n                {%- endfor -%}\r\n                {%- for item in items1 if item.section_title == section  -%}\r\n                    <tr class=\"styleme{{outerloop}}\" style=\"background:#022856;color: white;text-align: right;\">\r\n                        <td colspan=\"6\" style=\"color: #ff9800; \">{{ item.amounts }} </td>\r\n                    </tr>\r\n                {%- endfor -%}\r\n        {%- endfor -%}\r\n    </tbody>\r\n\r\n</table>\r\n\r\n<hr>\r\n<h3>Bundle Items</h3>\r\n<table class=\"table table-sm  table-bordered table-condensed\">\r\n   <thead>\r\n\t   <tr>\r\n\t   <th scope=\"col\">Sr</th>\r\n\t\t<th scope=\"col\">Parent Item</th>\r\n\t   <th scope=\"col\">Item Code</th> \r\n\t   <th scope=\"col\">Item Name</th>\r\n\t   <th scope=\"col\">Description</th>\r\n\t   <th scope=\"col\">Qty</th>\r\n\t   <th scope=\"col\">UOM</th>\r\n\t   </tr>\r\n   </thead>\r\n   <tbody>\r\n\r\n\t{%- for item1 in doc.packed_items  -%}\r\n\t\t<tr  >\r\n\t\t    <td>{{ item1.idx }}</td>\r\n\t\t\t<td>{{ item1.parent_item }}</td>\r\n\t\t\t<td>{{ item1.item_code }}</td>\r\n\t\t\t<td>{{ item1.item_name }}</td>\r\n\t\t\t<td>{{ item1.description }}</td>\r\n\t\t\t<td>{{ item1.qty }}</td>\r\n\t\t\t<td> {{item1.uom }} </td>\r\n\t\t</tr>\r\n\t{%- endfor -%}\r\n   </tbody>\r\n</table>\r\n\r\n<div class=\"row section-break\" data-label=\"\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t{%- if doc.base_total -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Total (Company Currency): </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\"> {{doc.get_formatted(\"base_total\", doc) }}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n</div>\r\n<div class=\"row section-break\" data-label=\"\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t{%- if doc.net_total -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"address_display\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Net Total: </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\"> {{doc.get_formatted(\"net_total\", doc) }}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n</div>\r\n<div class=\"row section-break\" data-label=\"\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t{%- if doc.base_grand_total -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Grand Total (Company Currency): </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\"> {{doc.get_formatted(\"base_grand_total\", doc) }}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n</div>\r\n<div class=\"row section-break\" data-label=\"\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t{%- if doc.terms -%}\r\n\t\t<div class=\"row  data-field\" data-fieldname=\"\" data-fieldtype=\"Small Text\">\r\n\t\t\t<div class=\"col-xs-5\">\r\n\t\t\t\t<label>Term Details: </label>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-xs-7 value\"> {{doc.terms}}</div>\r\n\t\t</div>\r\n\t\t{%- endif -%}\r\n\t</div>\r\n</div>",
